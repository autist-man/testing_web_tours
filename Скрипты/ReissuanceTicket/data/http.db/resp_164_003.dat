@font-face
{
	font-family: 'FontIcons';
	font-weight: normal;
	font-style: normal;
	src: url('./_rsc_icons/_rsc_icons.eot'); /* IE9 Compat Modes */
	src: url('./_rsc_icons/_rsc_icons.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('./_rsc_icons/_rsc_icons.woff') format('woff'),
	url('./_rsc_icons/_rsc_icons.ttf') format('truetype');
			
}

@font-face
{
	font-family: 'MetricRegular';
	font-weight: normal;
	font-style: normal;
	src: url('./_rsc_fonts/Regular/MetricNEW-Web-Regular.eot'); /* IE9 Compat Modes */
	src: url('./_rsc_fonts/Regular/MetricNEW-Web-Regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
   	url('./_rsc_fonts/Regular/MetricNEW-Web-Regular.woff') format('woff'),
	url('./_rsc_fonts/Regular/MetricNEW-App-Regular.ttf') format('truetype'),
	url('./_rsc_fonts/Regular/MetricNEW-Desktop-Regular.otf') format('opentype');
} 


